
@{
    ViewData["Title"] = "Error";
}

<h1>Error page</h1>

@if (ViewData["ErrorMsg"] != null)
{
    <h2>@ViewData["ErrorMsg"]</h2>
}
else
{
    <p>Something bad happened. Please try again later.</p>
}
<img src="~/images/NotFoundImage.jpg" alt="Guy looking through magnifying glass" height="200" class="img-thumbnail rounded"/>
